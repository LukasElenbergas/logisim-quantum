<?xml version="1.0" encoding="UTF-8"?>
<!-- Change $PATH_TO_CLONED_PROJECT to where you cloned the project on your system-->
<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  <jar>$PATH_TO_CLONED_PROJECT\logisim-quantum\out\artifacts\logisim-quantum.jar</jar>
  <outfile>$PATH_TO_CLONED_PROJECT\logisim-quantum\out\artifacts\logisim-quantum.exe</outfile>
  <errTitle></errTitle>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl></downloadUrl>
  <supportUrl></supportUrl>
  <stayAlive>false</stayAlive>
  <restartOnCrash>false</restartOnCrash>
  <manifest></manifest>
  <icon>$PATH_TO_CLONED_PROJECT\logisim-quantum\src\main\resources\logisim\img\logisim-icon-128.ico</icon>
  <classPath>
    <mainClass>com.cburch.logisim.Main</mainClass>
  </classPath>
  <jre>
    <path>%JAVA_HOME%;%PATH%</path>
    <requiresJdk>false</requiresJdk>
    <requires64Bit>false</requires64Bit>
    <minVersion>22</minVersion>
    <maxVersion></maxVersion>
  </jre>
</launch4jConfig>